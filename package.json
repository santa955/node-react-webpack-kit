{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "nodemon -w src --exec \"babel-node ./src/app.js\"",
    "build": "npm run clean && babel src --out-dir ./dist --source-maps",
    "clean": "rimraf dist",
    "serve": "node ./dist/app.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "highlight.js": "^9.15.6",
    "http-errors": "~1.6.2",
    "markdown-it": "^8.4.2",
    "morgan": "^1.9.1",
    "nunjucks": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/runtime": "^7.4.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3"
  }
}
